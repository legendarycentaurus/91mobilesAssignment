<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
	pageEncoding="ISO-8859-1"%>
<div ng-controller="homeCtrl">
	<div >
		<ul class="product" ng-repeat="product in laptop">
			<li>{{product.productCategory}}</li>
			<li>{{product.description}}</li>
		</ul>

	</div>
	
	<div class="row" style="overflow-x: scroll;" >
	<h3>Phone</h3>
  <div class="col-sm-6 col-md-4" ng-repeat="product in phone">
    <div class="thumbnail">
      <img class="thumbnail-size" src="https://mobile.panasonic.com/content/products/eluga-z1-pro/gallery/eluga-z1-pro-mobiles.jpg" alt="...">
      <div class="caption">
        <h5>{{product.description}}<span class="right">Price:{{product.price | currency }}</span></h5>
        <p></p>
        <p><a  class="btn btn-primary form-control input-lg"  ng-click="productDetails(product)" role="button">Add To Cart</a> </p>
      </div>
    </div>
  </div>
</div>

<div class="row" style="overflow-x: scroll;" >
	<h3>Laptop</h3>
  <div class="col-sm-6 col-md-4" ng-repeat="product in laptop">
    <div class="thumbnail">
      <img class="thumbnail-size" src="data:image/jpeg;base64," alt="...">
      <div class="caption">
        <h5>{{product.description}}<span class="right">Price:{{product.price | currency }}</span></h5>
        <p></p>
        <p><a href="#" class="btn btn-primary form-control input-lg" role="button">Add To Cart</a> </p>
      </div>
    </div>
  </div>
</div>
	
</div>
